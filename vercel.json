{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
   "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}